<div class="col-12">
  <div class="fileters-wrapper">
    {% if content['#company'] is not empty %}
      <div class="form-group">
        <label for="company">Select Subsidiary Company</label>
        <select name="company" id="company" class="form-control">
          {% for comp in content['#company'] %}
            <option value="{{ comp }}">{{ comp }}</option>
          {% endfor %}
        </select>
      </div>
    {% endif %}
    <div class="form-group">
      <label for="year">Select Year</label>
      <select name="year" id="year" class="form-control">
        {% for year in content['#options'] %}
          <option value="{{ year }}">{{ year }}</option>
        {% endfor %}
      </select>
    </div>
  </div>
</div>
<div class="col-12">
  <ul class="financial-results-list no-bullets">
    {% for item in content['#data_obj'] %}
      <li><a href="{{ item.file }}" target="_blank">{{ item.title }}</a></li>
    {% endfor %}
  </ul>
</div>
